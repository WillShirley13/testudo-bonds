{
  "rust-analyzer.cargo.buildScripts.enable": true,
  "rust-analyzer.cargo.buildScripts.overrideCommand": [
    "cargo",
    "check",
    "--quiet",
    "--workspace",
    "--message-format=json",
    "--all-targets"
  ],
  "rust-analyzer.server.extraEnv": {
    "CARGO_INCREMENTAL": "0",
    "OPENSSL_NO_VENDOR": "1"
  },
  "rust-analyzer.cargo.extraEnv": {
    "CARGO_INCREMENTAL": "0",
    "OPENSSL_NO_VENDOR": "1"
  }
}